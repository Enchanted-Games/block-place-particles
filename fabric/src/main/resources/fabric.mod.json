{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "provides": [
        "enchanted-vertical-slabs"
    ],
    "version": "${mod_version}",
    "name": "${mod_name}",
    "description": "${description}",
    "authors": [
        "${mod_author}"
    ],
    "contact": {
        "homepage": "${mod_homepage}",
        "sources": "${mod_source}",
        "issues": "${mod_issues}"
    },
    "license": "${license}",
    "icon": "assets/enchanted-vertical-slabs/icon.png",
    "environment": "*",
    "entrypoints": {
        "client": [
          "games.enchanted.blockplaceparticles.FabricClientEntrypoint"
        ]
    },
    "mixins": [
        "${mod_id}.mixins.json",
        "${mod_id}.fabric.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=${fabric_loader_version}",
        "fabric-api": "*",
        "minecraft": ${minecraft_fabric_version_range},
        "java": ">=${java_version}"
    },
    "suggests": {
        "another-mod": "*"
    }
}
  